{
  "name": "fampay-assignment",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "commit": "git cz",
    "dev": "nodemon ./bin/www",
    "lint": "eslint . --quiet",
    "lint-fix": "eslint . --fix",
    "pretty-quick": "pretty-quick",
    "start": "node ./bin/www"
  },
  "dependencies": {
    "cookie-parser": "^1.4.5",
    "dayjs": "^1.10.5",
    "debug": "^4.3.1",
    "dotenv-defaults": "^2.0.2",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "googleapis": "^78.0.0",
    "http-errors": "^1.8.0",
    "http-status-codes": "^2.1.4",
    "mongoose": "^5.12.14",
    "mongoose-fuzzy-searching": "^2.0.2",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@commitlint/cz-commitlint": "^12.1.4",
    "@commitlint/prompt": "^12.1.4",
    "commitizen": "^4.2.4",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-xo": "^0.37.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.1",
    "pretty-quick": "^3.1.1"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "npm run lint",
      "pre-commit": "pretty-quick --staged"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@commitlint/cz-commitlint"
    }
  }
}
